@model UdemyMVC5UltimateGuide.Identity.ApplicationUser
@{
    ViewBag.Title = "MyProfile";
    
}

<h2>MyProfile</h2>
<a class="nav-link" href="/home/index">Back</a>
<table class="table-bordered">
    <tr>
        <th>Username</th>
        <td>@Model.UserName</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>PhoneNumber</th>
        <td>@Model.PhoneNumber</td>
    </tr>
    <tr>
        <th>Birthday</th>
        <td>@Model.Birthday</td>
    </tr>
    <tr>
        <th>City</th>
        <td>@Model.City</td>
    </tr>
    <tr>
        <th>Country</th>
        <td>@Model.Country</td>
    </tr>
</table>